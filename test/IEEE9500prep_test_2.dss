//Lowered Source Voltage
Edit Vsource.source Pu=1.01
//Define yearly loadshape
New "LoadShape.LoadShape4" npts=8760 interval=1.0 mult=(File=LoadShape4.csv) Action=Normalize
//Constant loading for steamgen1
new loadshape.steamgen1 npts=2 interval=0 hour=[0.00,8760.00] mult=[750.0,750.0] qmult=[562.5, 562.5] useactual=yes
edit Generator.SteamGen1 daily=steamgen1 model=1

//Deactivate batteries (replace with load)
edit Storage.Battery1 enabled=no
edit Storage.Battery2 enabled=no

//New load shape for each battery
new loadshape.bat1 npts=8760 interval=1.0 mult=(file=batt_setting/hca9500node_Mon_battmeter1_1.csv col=3, header=yes) qmult=(file=batt_setting/hca9500node_Mon_battmeter1_1.csv col=4, header=yes) useactual=yes
new loadshape.bat2 npts=8760 interval=1.0 mult=(file=batt_setting/hca9500node_Mon_battmeter2_1.csv col=3, header=yes) qmult=(file=batt_setting/hca9500node_Mon_battmeter2_1.csv col=4, header=yes) useactual=yes

//create load
new load.bat1 phases=3 bus1=m2001-ESS1 kW=250 pf=1.0 kV=0.480 mode=1 daily=bat1 yearly=bat1
new load.bat2 phases=3 bus1=m2001-ESS2 kW=250 pf=1.0 kV=0.480 mode=1 daily=bat2 yearly=bat2

//storage monitors
new monitor.bat1 element=load.bat1  terminal=1 mode=65 ppolar=no
new monitor.bat2 element=load.bat2  terminal=1 mode=65 ppolar=no

